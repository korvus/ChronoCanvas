{
  "name": "ChronoCanvas",
  "version": "0.0.1",
  "description": "Allow to display a visual Chronometer in a canvas tag, associate to its ID",
  "main": "index.js",
  "scripts": {
    "build": "browserify rootFile.js --standalone chronoCanvas --outfile index.js",
    "watch": "watchify rootFile.js --standalone chronoCanvas --outfile index.js",
    "watchExCommonJs": "watchify examples/chronoCanvas-commonJS.js --outfile examples/chronoCanvas-commonJS-bundle.js",
    "watchExAMD": "watchify examples/chronoCanvas-AMD.js --outfile examples/chronoCanvas-AMD-bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/korvus/ChronoCanvas.git"
  },
  "keywords": [
    "canvas",

    "node",
    "javascript"
  ],
  "author": "korvus (Simon Ertel)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/korvus/ChronoCanvas/issues"
  },
  "homepage": "https://github.com/korvus/ChronoCanvas",
  "dependencies": {
    "browserify": "~6.3.2",
    "watchify": "~2.1.1"
  }
}
